{
    "name": "pondjs",
    "version": "1.0.0-alpha.10",
    "description": "A TimeSeries library built on Immutable.js with Typescript",
    "main": "lib/exports.js",
    "types": "lib/exports.d.ts",
    "author": "Software Engineering Group at ESnet",
    "license": "BSD-style",
    "engines": {
        "node": ">=8"
    },
    "dependencies": {
        "immutable": "4.0.0-rc.12",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.202",
        "jest": "^29.7.0",
        "prettier": "^1.17.0",
        "ts-jest": "^29.2.5",
        "tslint": "^5.16.0",
        "tslint-config-prettier": "^1.18.0",
        "typedoc": "^0.14.2",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "build": "tsc --version && tsc",
        "watch": "tsc --watch",
        "docs": "typedoc --json ../website/src/doc.json --mode modules ./src/*.ts --ignoreCompilerErrors",
        "test": "jest --watch",
        "lint": "tslint ./src/*.ts"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(/tests/.*)\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    }
}
